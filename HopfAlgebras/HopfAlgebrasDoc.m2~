doc ///
   Key
      "Creating a bialgebra"
   Description
      Text
         PARAGRAPH
      Example
         A = QQ{x,y,z}
         I = ncIdeal {x^2 - 1_A, y^2 - 1_A, x*y - y*x, z*x - y*z, z*y - x*z, z^2 - (1/2)*(1_A + x + y - x*y)}
         Igb = ncGroebnerBasis(I, InstallGB => true)
         B = A/I
         H = bialgebra(B,{{(x,x)},{(y,y)},{(1/2*z,z),(1/2*z,x*z),(1/2*y*z,z),(-1/2*y*z,x*z)}},{1_B,1_B,1_B})
///
   
